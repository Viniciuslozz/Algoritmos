algoritmo "calculo_area_retangulo"
var
   base, altura, area: real
inicio
   escreval("=== CALCULO DA AREA DE UM RETANGULO ===")
   escreva("Digite a base: ")
   leia(base)
   escreva("Digite a altura: ")
   leia(altura)
   area <- base * altura
   escreval("A area do retangulo é: ", area)
   escreval("======================================")
fimalgoritmo